<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suvenconsultants.sctple_learn">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/image"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WebDev"
            android:label="@string/title_activity_web_dev"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvenconsultants.sctple_learn.MainActivity" />
        </activity>
        <activity
            android:name=".Java"
            android:label="@string/title_activity_java"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvenconsultants.sctple_learn.MainActivity" />
        </activity>
        <activity
            android:name=".Database"
            android:label="@string/title_activity_database"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvenconsultants.sctple_learn.MainActivity" />
        </activity>
        <activity
            android:name=".Androids"
            android:label="Android Development"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvenconsultants.sctple_learn.MainActivity" />
        </activity>
        <activity
            android:name=".Cart"
            android:label="Add to Cart"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Checkout"
            android:label="CheckOut"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".LastActivity"
            android:label="SCTPL e-Learn"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".More"
            android:label="More Courses"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvenconsultants.sctple_learn.MainActivity"/>
        </activity>
    </application>

</manifest>